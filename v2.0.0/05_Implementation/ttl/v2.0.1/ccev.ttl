#################################################################
#    Object Properties
#################################################################


###  http://data.europa.eu/m8g/cccev#code
:code rdf:type owl:ObjectProperty ;
         rdfs:domain :CriterionPropertyDatum ;
         rdfs:range skos:Concept ;
         rdfs:label "has Code"@en .
###  http://data.europa.eu/m8g/cccev#criterionGroupPropertyType
:criterionGroupPropertyType rdf:type owl:ObjectProperty ;
                               rdfs:domain :CriterionPropertyGroup ;
                               rdfs:range skos:Concept ;
                               skos:note: "See code list 'CriterionGroupPropertyType'"@en ;
                               rdfs:label "has Criterion Group Property Type"@en .

 ###  http://data.europa.eu/m8g/cccev#criterionProperty
:criterionProperty rdf:type owl:ObjectProperty ;
                      rdfs:domain :CriterionPropertyGroup ;
                      rdfs:range :CriterionProperty ;
                      rdfs:label "has Criterion Property"@en .
                      

 ###  http://data.europa.eu/m8g/cccev#criterionPropertyGroup
:criterionPropertyGroup rdf:type owl:ObjectProperty ;
                           rdfs:domain :Criterion ;
                           rdfs:range :CriterionPropertyGroup ;
                           rdfs:label "has Criterion Property Group"@en .                     


###  http://data.europa.eu/m8g/cccev#evaluationMethodType
:evaluationMethodType rdf:type owl:ObjectProperty ;
                         rdfs:domain :Criterion ;
                         rdfs:range skos:Concept ;
                         skos:note: "See code list 'EvaluationMethodType'"@en ;
                         rdfs:label "has Evaluation Method Type"@en .   
                         

###  http://data.europa.eu/m8g/cccev#legalBasisIdentifier
:legalBasisIdentifier rdf:type owl:ObjectProperty ;
                         rdfs:domain :Legislation ;
                         rdfs:range skos:Concept ;
                         skos:note: "See code list 'LegalBasisIdentifier'"@en ;
                         rdfs:label "has Legal Basis Identifier"@en .


###  http://data.europa.eu/m8g/cccev#legislation
:legislation rdf:type owl:ObjectProperty ;
                rdfs:domain :Criterion ;
                rdfs:range :Legislation ;
                rdfs:label "has Legislation"@en .


###  http://data.europa.eu/m8g/cccev#legislativeLevel
:legislativeLevel rdf:type owl:ObjectProperty ;
                     rdfs:domain :Legislation ;
                     rdfs:range skos:Concept ;
                     rdfs:label "has Legislative Level"@en .


###  http://data.europa.eu/m8g/cccev#period
:period rdf:type owl:ObjectProperty ;
           rdfs:domain :CriterionPropertyDatum ;
           rdfs:range ubl:Period ;
           rdfs:label "has Period"@en .


###  http://data.europa.eu/m8g/cccev#periodDatum
:periodDatum rdf:type owl:ObjectProperty ;
                rdfs:domain :CriterionPropertyDatum ;
                rdfs:range ubl:Period ;
                rdfs:label "has Period Datum"@en .


###  http://data.europa.eu/m8g/cccev#propertySubgroup
:propertySubgroup rdf:type owl:ObjectProperty ;
                     rdfs:domain :CriterionPropertyGroup ;
                     rdfs:range :CriterionPropertyGroup ;
                     rdfs:label "has Property Subgroup"@en .


###  http://data.europa.eu/m8g/cccev#quantity
:quantity rdf:type owl:ObjectProperty ;
             rdfs:domain :CriterionPropertyDatum ;
             rdfs:range ccts:Quantity ;
             rdfs:label "has Quantity"@en .


###  http://data.europa.eu/m8g/cccev#subCriterion
:subCriterion rdf:type owl:ObjectProperty ;
                 rdfs:domain :Criterion ;
                 rdfs:range :Criterion ;
                 rdfs:label "has Sub Criterion"@en .


###  http://data.europa.eu/m8g/cccev#proofsCriterion
:proofsCriterion rdf:type owl:ObjectProperty ;
                 owl:inverseOf :providesEvidence ;
                 rdfs:domain :Evidence ;
                 rdfs:range :Criterion ;
                 rdfs:label "proofs Criterion"@en .


###  http://data.europa.eu/m8g/cccev#providesEvidence
:providesEvidence rdf:type owl:ObjectProperty ;
                  rdfs:domain :Criterion ;
                  rdfs:range :Evidence ;
                  rdfs:label "provides Evidence"@en .


###  http://data.europa.eu/m8g/cccev#refinesRequirement
:refinesRequirement rdf:type owl:ObjectProperty ;
                    rdfs:domain :CriterionPropertyDatum ;
                    rdfs:range :CriterionProperty ;
                    owl:propertyDisjointWith :respondsQuestion ;
                    rdfs:label "refines Requirement"@en .


###  http://data.europa.eu/m8g/cccev#respondsQuestion
:respondsQuestion rdf:type owl:ObjectProperty ;
                  rdfs:domain :CriterionPropertyDatum ;
                  rdfs:range :CriterionProperty ;
                  rdfs:label "responds Question"@en .

###  http://data.europa.eu/m8g/cccev#asksForData
:asksForData rdf:type owl:ObjectProperty ;
             owl:inverseOf :respondsQuestion ;
             rdfs:domain :CriterionProperty ;
             rdfs:range :CriterionPropertyDatum ;
             owl:propertyDisjointWith :expressesRequirement ;
             rdfs:label "asks For Data"@en .


###  http://data.europa.eu/m8g/cccev#expressesRequirement
:expressesRequirement rdf:type owl:ObjectProperty ;
                      owl:inverseOf :refinesRequirement ;
                      rdfs:domain :CriterionProperty ;
                      rdfs:range :CriterionPropertyDatum ;
                      rdfs:label "expresses Requirement"@en .


###  http://data.europa.eu/m8g/cccev#amount
:amount rdf:type owl:ObjectProperty ;
           rdfs:domain :CriterionPropertyDatum ;
           rdfs:range ccts:Amount ;
           rdfs:label "has Amount"@en .


###  http://data.europa.eu/m8g/cccev#confidentialityLevelType
:confidentialityLevelType rdf:type owl:ObjectProperty ;
                             rdfs:domain :Evidence ;
                             rdfs:range skos:Concept ;
                             skos:note: "See code list 'ConfidentialityLevelType'"@en ;
                             rdfs:label "has Confidentiality Level Type"@en .


###  http://data.europa.eu/m8g/cccev#identifier
:identifier rdf:type owl:ObjectProperty ;
               rdfs:domain :Criterion ,
                           :CriterionProperty ,
                           :CriterionPropertyDatum ,
                           :CriterionPropertyGroup ;
               rdfs:range ccts:Identifier ;
               rdfs:label "has Identifier"@en .


###  http://data.europa.eu/m8g/cccev#uri
:uri rdf:type owl:ObjectProperty ;
        rdfs:domain :CriterionPropertyDatum ,
                    :Legislation ;
        rdfs:range ccts:URI ;
        rdfs:label "has URI"@en .


#################################################################
#    Data properties
#################################################################

###  http://data.europa.eu/m8g/cccev#criterionPropertyDatumDate
:criterionPropertyDatumDate rdf:type owl:DatatypeProperty ;
                               rdfs:domain :CriterionPropertyDatum ;
                               rdfs:range xsd:dateTime ;
                               rdfs:label "has Criterion Property Datum Date"@en .


###  http://data.europa.eu/m8g/cccev#criterionPropertyDescription
:criterionPropertyDescription rdf:type owl:DatatypeProperty ;
                                 rdfs:domain :CriterionPropertyGroup ;
                                 rdfs:range xsd:string ;
                                 rdfs:label "has Criterion Property Description"@en .


###  http://data.europa.eu/m8g/cccev#indicator
:indicator rdf:type owl:DatatypeProperty ;
              rdfs:domain :CriterionPropertyDatum ;
              rdfs:range xsd:boolean ;
              rdfs:label "has Indicator"@en .


###  http://data.europa.eu/m8g/cccev#objective
:objective rdf:type owl:DatatypeProperty ;
              rdfs:domain :Criterion ;
              rdfs:range xsd:boolean ;
              rdfs:label "has Objective"@en .


###  http://data.europa.eu/m8g/cccev#text
:text rdf:type owl:DatatypeProperty ;
         rdfs:domain :CriterionPropertyDatum ;
         rdfs:range xsd:string ;
         rdfs:label "has Text"@en .


###  http://data.europa.eu/m8g/cccev#time
:time rdf:type owl:DatatypeProperty ;
         rdfs:domain :CriterionPropertyDatum ;
         rdfs:range xsd:dateTime ;
         rdfs:label "has Time"@en .


###  http://data.europa.eu/m8g/cccev#weight
:weight rdf:type owl:DatatypeProperty ;
           rdfs:domain :Criterion ;
           rdfs:range xsd:decimal ;
           rdfs:label "has Weight"@en .


###  http://data.europa.eu/m8g/cccev#weightingConsiderationDescription
:weightingConsiderationDescription rdf:type owl:DatatypeProperty ;
                                      rdfs:domain :Criterion ;
                                      rdfs:range xsd:string ;
                                      rdfs:label "has Weighting Consideration Description"@en .


###  http://data.europa.eu/m8g/cccev#article
:article rdf:type owl:DatatypeProperty ;
            rdfs:domain :Legislation ;
            rdfs:range xsd:string ;
            rdfs:label "has Article"@en .


###  http://data.europa.eu/m8g/cccev#description
:description rdf:type owl:DatatypeProperty ;
                rdfs:domain :Criterion ,
                            :CriterionProperty ,
                            :Legislation ;
                rdfs:range xsd:string ;
                rdfs:label "has Description"@en .


###  http://data.europa.eu/m8g/cccev#name
:name rdf:type owl:DatatypeProperty ;
         rdfs:domain :Criterion ,
                     :CriterionProperty ,
                     :CriterionPropertyGroup ;
         rdfs:range xsd:string ;
         rdfs:label "has Name"@en .


###  http://data.europa.eu/m8g/cccev#title
:title rdf:type owl:DatatypeProperty ;
          rdfs:domain :Legislation ;
          rdfs:range xsd:string ;
          rdfs:label "has Title"@en .




###  http://data.europa.eu/m8g/cccev#Criterion
:Criterion rdf:type owl:Class ;
                                                    skos:definition "A condition that needs to be answered for evaluation purposes."@en ;
                                                    rdfs:label "Criterion"@en .


###  http://data.europa.eu/m8g/cccev#CriterionProperty
:CriterionProperty rdf:type owl:Class ;
                                                            skos:definition "Specific information needed to measure a criterion."@en ;
                                                            rdfs:label "Criterion Property"@en .


###  http://data.europa.eu/m8g/cccev#CriterionPropertyDatum
:CriterionPropertyDatum rdf:type owl:Class ;
                                                                 skos:definition "The value  provided for a criterion property."@en ;
                                                                 rdfs:label "Criterion Property Datum"@en .


###  http://data.europa.eu/m8g/cccev#CriterionPropertyGroup
:CriterionPropertyGroup rdf:type owl:Class ;
                                                                 skos:definition "An organised structure of related criterion properties."@en ;
                                                                 rdfs:label "Criterion Property Group"@en .


###  http://data.europa.eu/m8g/cccev#Evidence
:Evidence rdf:type owl:Class ;
                                                   rdfs:label "Evidence"@en .


###  http://data.europa.eu/m8g/cccev#Legislation
:Legislation rdf:type owl:Class ;
                                                      skos:definition "The provision which confers competence to adopt the act in question."@en ;
                                                      rdfs:label "Legislation"@en .
