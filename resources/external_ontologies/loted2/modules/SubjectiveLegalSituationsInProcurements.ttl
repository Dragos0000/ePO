                                                                                               owl:allValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                                                                          owl:unionOf ( ProcurementsTopClasses:LegalPerson
                                                                                                                                                        ProcurementsTopClasses:NaturalPerson
                                                                                                                                                      )
                                                                                                                                        ]
                                                                                                                                        [ rdf:type owl:Restriction ;
